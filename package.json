{
  "name": "ping-pong-bot",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "NODE_OPTIONS=--max_old_space_size=4096 nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug",
    "start:prod": "node dist/src/app/app",
    "indexer": "nest start --config ./nest-cli-indexer.json",
    "indexer:dev": "NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=development nest start --config ./nest-cli-indexer.json --watch",
    "indexer:debug": "nest start --debug",
    "indexer:prod": "node dist/src/indexer/indexer",
    "lint": "NODE_OPTIONS=--max_old_space_size=4096 eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_OPTIONS=--max_old_space_size=4096 jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/bull": "^10.2.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "aws-sdk": "^2.1630.0",
    "axios": "^1.6.8",
    "bull": "^4.16.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cron": "^3.1.7",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nest-winston": "^1.9.7",
    "pg": "^8.12.0",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.20",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.1",
    "@types/multer": "^1.4.11",
    "@types/node": "18.16.12",
    "@types/passport-google-oauth20": "^2.0.15",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
